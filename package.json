{
  "name": "dag-editor",
  "displayName": "DAG Editor",
  "description": "A custom document editor extension for json-formatted DAG files.",
  "repository": {
    "type": "git",
    "url": "https://github.com/QinXiaodong/DAG-Editor.git"
  },
  "publisher": "QinXiaodong",
  "icon": "assets/icon.png",
  "version": "0.0.10",
  "engines": {
    "vscode": "^1.75.0"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "dag-editor",
        "displayName": "DAG Editor",
        "selector": [
          {
            "filenamePattern": "*.dag.json"
          }
        ],
        "priority": "default"
      }
    ],
    "commands": [
      {
        "command": "dag-editor.new",
        "title": "New DAG",
        "category": "DAG Editor"
      }
    ],
    "icon": "assets/icon.png"
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "node ./esbuild.js",
    "package": "node ./esbuild.js",
    "watch": "node ./esbuild.js --watch",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@types/vscode-webview": "^1.57.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "esbuild": "^0.16.10",
    "eslint": "^8.26.0",
    "glob": "^8.0.3",
    "prettier": "^2.8.1",
    "tslib": "^2.6.3",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@antv/g6": "^5.0.6"
  }
}